package test0;

import java.util.Scanner;

class imsi {
	  public static void main(String args[]) {
		Scanner n = new Scanner(System.in);
		  
		System.out.println(n);
	    if (n == n) {
		clearScreen();
	    }
	  }


	  public static void clearScreen() {
	    for (int i = 0; i < 80; i++)
	      System.out.println("");
	  }

}

