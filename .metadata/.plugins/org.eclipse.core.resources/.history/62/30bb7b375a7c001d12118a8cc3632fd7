package test0;

import java.util.Scanner;

class imsi {
	  public static void main(String args[]) {
		Scanner n = new Scanner(System.in);
		 String aa = n.nextLine();
		
		 System.out.println(aa);
	   
		clearScreen();
	 
	  }


	  public static void clearScreen() {
	    for (int i = 0; i < 80; i++)
	      System.out.println("");
	  }

}

